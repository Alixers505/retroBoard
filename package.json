{
  "name": "retroboard",
  "version": "1.0.0",
  "description": "A retrospective board application for team meetings",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alixers505/retroboard.git"
  },
  "author": "Alysha Buselt",
  "license": "SEE LICENSE IN README.md",
  "bugs": {
    "url": "https://github.com/Alixers505/retroboard/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "sass": "^1.19.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
